{
  "command_timeout": 5,
  "templates_folder": "templates/create/used",
  "templates_pairs": [
    [
      "",
      ""
    ]
  ],
  "trigger_on_file_creation": true,
  "auto_jump_to_cursor": true,
  "enable_system_commands": false,
  "shell_path": "",
  "user_scripts_folder": "templates/scripts",
  "enable_folder_templates": true,
  "folder_templates": [
    {
      "folder": "periodic/daily",
      "template": "templates/create/periodic/daily template.md"
    },
    {
      "folder": "periodic/weekly",
      "template": "templates/create/periodic/weekly template.md"
    },
    {
      "folder": "periodic/monthly",
      "template": "templates/create/periodic/monthly template.md"
    },
    {
      "folder": "periodic/quarterly",
      "template": "templates/create/periodic/quarterly template.md"
    },
    {
      "folder": "periodic/yearly",
      "template": "templates/create/periodic/yearly template.md"
    },
    {
      "folder": "base/categories",
      "template": "templates/create/system/category template.md"
    },
    {
      "folder": "base/_hierarchy",
      "template": "templates/create/system/hierarchy template.md"
    },
    {
      "folder": "base/_meta-notes",
      "template": "templates/create/system/meta-note template.md"
    },
    {
      "folder": "base/_problems",
      "template": "templates/create/system/problem template.md"
    },
    {
      "folder": "base/creators",
      "template": "templates/create/people/creator template.md"
    },
    {
      "folder": "base/contacts",
      "template": "templates/create/people/contact template.md"
    },
    {
      "folder": "base/productions",
      "template": "templates/create/people/production template.md"
    },
    {
      "folder": "base/additions",
      "template": "templates/create/additions/addition template.md"
    },
    {
      "folder": "base/notes",
      "template": "templates/create/notes/default note template.md"
    },
    {
      "folder": "sources",
      "template": "templates/create/sources/source template.md"
    },
    {
      "folder": "projects/short",
      "template": "templates/create/projects/short project template.md"
    },
    {
      "folder": "projects/single",
      "template": "templates/create/projects/single project template.md"
    }
  ],
  "enable_file_templates": false,
  "file_templates": [
    {
      "regex": ".*",
      "template": ""
    }
  ],
  "syntax_highlighting": true,
  "syntax_highlighting_mobile": false,
  "enabled_templates_hotkeys": [
    "templates/create/additions/addition template.md",
    "templates/create/additions/meeting template.md",
    "templates/create/additions/experiment template.md",
    "templates/create/additions/report template.md"
  ],
  "startup_templates": [
    "templates/onStartup.md"
  ],
  "intellisense_render": 1,
  "enable_ribbon_icon": false
}