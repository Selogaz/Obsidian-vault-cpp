filters:
  and:
    - or:
        - file.hasTag("note/basic/seed")
        - file.hasTag("note/basic/fern")
        - file.hasTag("note/basic/incubator")
        - file.hasTag("note/basic/evergreen")
    - or:
        - this.evergreen_title == null
        - this.evergreen_title == ""
        - this.evergreen_title.length == 0
        - file.name.contains(this.evergreen_title)
        - aliases.join(" ").contains(this.note_title)
    - or:
        - this.evergreen_type == null
        - this.evergreen_type == ""
        - this.evergreen_type.length == 0
        - file.tags.containsAny(this.evergreen_type)
formulas:
  type: tags.filter(value.startsWith("note/basic/"))[0].replace(/^.*\//, '')
  backlinks: |
    file.backlinks.length
  outlinks: file.links.length
properties:
  formula.type:
    displayName: üóÑÔ∏è type
  file.name:
    displayName: üî§ title
  formula.backlinks:
    displayName: ‚¨áÔ∏è
  formula.outlinks:
    displayName: ‚¨ÜÔ∏è
views:
  - type: table
    name: üå≤ Evergreen
    filters:
      and:
        - '!file.hasTag("note/discourse/question")'
    order:
      - formula.type
      - formula.backlinks
      - formula.outlinks
      - file.name
    limit: 28
  - type: table
    name: ‚ùì Favorite Problems
    filters:
      and:
        - file.hasTag("note/discourse/question")
    order:
      - formula.type
      - formula.backlinks
      - formula.outlinks
      - file.name
    limit: 28
