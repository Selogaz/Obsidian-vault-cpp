filters:
  and:
    - file.hasTag("note")
    - file.hasTag("category/" + this.file.name.replace(" ", "_"))
    - or:
        - this.note_title == null
        - this.note_title == ""
        - this.note_title.length == 0
        - file.name.contains(this.note_title)
        - aliases.join(" ").contains(this.note_title)
    - or:
        - this.note_tags == null
        - this.note_tags == ""
        - this.note_tags.length == 0
        - file.tags.join(" ").contains(this.note_tags)
formulas:
  backlinks: |
    file.backlinks.length
  outlinks: file.links.length
