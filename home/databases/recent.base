filters:
  and:
    - file.folder != "classes"
    - file.folder != "files"
    - file.folder != "home"
    - file.folder != "periodic/statuses"
    - file.folder != "templates"
    - or:
        - this.recent_title == null
        - this.recent_title == ""
        - this.recent_title.length == 0
        - file.name.contains(this.recent_title)
        - aliases.join(" ").contains(this.recent_title)
