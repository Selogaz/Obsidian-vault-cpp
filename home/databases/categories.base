filters:
  and:
    - file.hasTag("system/category")
formulas:
  backlinks: |
    file.backlinks.length
  outlinks: file.links.length
properties:
  file.name:
    displayName: 🗺️ category
  note.relevant:
    displayName: 📌
  formula.backlinks:
    displayName: ⬇️
  formula.outlinks:
    displayName: ⬆️
views:
  - type: table
    name: Table
    order:
      - file.name
      - relevant
      - formula.backlinks
      - formula.outlinks
    limit: 28
